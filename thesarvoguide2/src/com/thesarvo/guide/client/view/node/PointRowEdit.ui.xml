<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:s='urn:import:com.thesarvo.guide.client.view.node'>

  <ui:with field='r' type='com.thesarvo.guide.client.view.res.Resources'/>
 
  <g:HTMLPanel tag='tr'> 

	<td>
		<s:BoundTextBox bindValue='@code' />
	</td>
	<td>
		<s:BoundTextBox bindValue='@description' width='300px'/>
	</td>
	<td>55G</td>
	<td>
		<s:BoundTextBox bindValue='@easting' />
	</td>
	<td>
		<s:BoundTextBox bindValue='@northing' />
	</td>	
	<td>
		<s:BoundTextBox bindValue='@height' />
	</td>
	<td>
		<g:Button ui:field='removeButton' >Remove</g:Button>
	</td>
  </g:HTMLPanel>
 
</ui:UiBinder>